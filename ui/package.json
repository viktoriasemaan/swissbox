{
  "name": "serverless-file-share-ui",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . && stylelint .",
    "lint:fix": "eslint . --fix && stylelint . --fix",
    "lint:js": "eslint .",
    "lint:css": "stylelint .",
    "config": "aws ssm get-parameter --name $(aws cloudformation describe-stacks --stack-name serverless-file-share --query \"Stacks[0].Outputs[?OutputKey=='WebConfig'].OutputValue\" --output text) --query \"Parameter.Value\" --output text > public/config.js",
    "clean": "rm -rf ./node_modules && rm -rf ./build && rm -rf ./public/config.js"
  },
  "dependencies": {
    "@aws-amplify/ui": "^5.6.8",
    "@aws-amplify/ui-react": "^5.0.6",
    "@aws-northstar/ui": "^1.1.7",
    "@cloudscape-design/components": "^3.0.342",
    "aws-amplify": "^5.3.5",
    "axios": "^1.6.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.14.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.8",
    "react-scripts": "5.0.1",
    "sass": "^1.64.1",
    "stylelint": "^15.10.2",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5"
    },
    "nth-check": "2.1.1",
    "postcss": "^8.4.31",
    "@adobe/css-tools": "^4.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
