{
  "name": "serverless-file-share-cdk",
  "version": "1.0.0",
  "license": "MIT",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "test": "jest",
    "build": "dotenv cdk synth",
    "deploy": "dotenv cdk deploy",
    "build:prod": "dotenv -e .env.prod cdk synth",
    "deploy:prod": "dotenv -e .env.prod cdk deploy",
    "watch": "dotenv cdk watch --hotswap-fallback",
    "local-invoke": "npm run build && sam local invoke ${1} --template cdk.out/serverless-file-share.template.json",
    "local-invoke:prod": "npm run build:prod && sam local invoke ${1} --template cdk.out/serverless-file-share.template.json",
    "destroy": "cdk destroy",
    "lint": "eslint .",
    "build:ui": "cd ../ui && npm install && npm run build && cd -",
    "clean": "rm -rf ./node_modules && rm -rf cdk.out",
    "nag": "cfn_nag_scan --input-path cdk.out/serverless-file-share.template.json"
  },
  "devDependencies": {
    "@aws-cdk/aws-glue-alpha": "^2.88.0-alpha.0",
    "@types/aws-lambda": "^8.10.86",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "aws-cdk": "^2.91.0",
    "aws-cdk-lib": "^2.87.0",
    "cdk-nag": "^2.27.100",
    "constructs": "^10.2.69",
    "dotenv-cli": "^7.0.0",
    "esbuild": "^0.17.14",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.2"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.6.0",
    "@aws-lambda-powertools/metrics": "^1.6.0",
    "@aws-lambda-powertools/tracer": "^1.6.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.363.0",
    "@aws-sdk/client-dynamodb": "^3.363.0",
    "@aws-sdk/client-s3": "^3.47.0",
    "@aws-sdk/credential-providers": "^3.303.0",
    "@aws-sdk/hash-node": "^3.357.0",
    "@aws-sdk/lib-dynamodb": "^3.388.0",
    "@aws-sdk/protocol-http": "^3.357.0",
    "@aws-sdk/s3-request-presigner": "^3.301.0",
    "@aws-sdk/url-parser": "^3.303.0",
    "@middy/core": "^4.2.8",
    "aws-xray-sdk": "^3.4.1",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  }
}
